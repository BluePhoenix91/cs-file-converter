<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<RootNamespace>CSFileConverter</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishAot>true</PublishAot>
		<InvariantGlobalization>true</InvariantGlobalization>

		<AssemblyName>CodeFileConverter</AssemblyName>
		<Product>Code File Converter</Product>
		<Description>Convert source code files from .NET, Python and Angular projects to text files</Description>
	</PropertyGroup>

</Project>
